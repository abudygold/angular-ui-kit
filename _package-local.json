{
	"name": "angular-ui",
	"version": "0.0.0",
	"scripts": {
		"ng": "ng",
		"start": "ng serve",
		"build": "ng build",
		"watch": "ng build --watch --configuration development",
		"test": "ng test",
		"build:lib": "ng build angular-ui-kit",
		"build:watch-lib": "ng build angular-ui-kit --watch"
	},
	"prettier": {
		"printWidth": 100,
		"singleQuote": true,
		"bracketSameLine": true,
		"useTabs": true,
		"tabWidth": 4,
		"htmlWhitespaceSensitivity": "strict",
		"plugins": [
			"prettier-plugin-organize-imports"
		],
		"overrides": [
			{
				"files": "*.html",
				"options": {
					"parser": "angular"
				}
			}
		]
	},
	"private": true,
	"packageManager": "npm@10.9.0",
	"dependencies": {
		"@angular/cdk": "~21.1.2",
		"@angular/common": "^21.1.0",
		"@angular/compiler": "^21.1.0",
		"@angular/core": "^21.1.0",
		"@angular/forms": "^21.1.0",
		"@angular/material": "~21.1.2",
		"@angular/platform-browser": "^21.1.0",
		"@angular/router": "^21.1.0",
		"@devkitify/angular-ui-kit": "file:dist/angular-ui-kit",
		"rxjs": "~7.8.0",
		"tslib": "^2.3.0"
	},
	"devDependencies": {
		"@angular/build": "^21.1.0",
		"@angular/cli": "^21.1.0",
		"@angular/compiler-cli": "^21.1.0",
		"@angular/material": "^21.1.2",
		"@tailwindcss/postcss": "^4.1.12",
		"jsdom": "^27.1.0",
		"ng-packagr": "^21.1.0",
		"postcss": "^8.5.3",
		"prettier": "^3.8.1",
		"prettier-plugin-organize-imports": "^4.3.0",
		"tailwindcss": "^4.1.12",
		"typescript": "~5.9.2",
		"vitest": "^4.0.8"
	}
}
